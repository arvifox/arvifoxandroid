<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="rectColor" format="color" />

    <declare-styleable name="RectView">
        <attr name="rectColor" />
    </declare-styleable>

    <attr name="rectViewStyle" format="reference" />
	
	<declare-styleable name="QualityProgressBar">
        <attr name="strokeWidth" format="dimension"/>
        <attr name="totalAnimationDuration" format="integer"/>
        <attr name="recolorAnimationDuration" format="integer"/>
        <attr name="text" format="string"/>
        <attr name="textColor" format="color"/>
        <attr name="maxTextSize" format="dimension"/>
        <attr name="rotation" format="integer"/>
        <attr name="colorUnspecified" format="color"/>
        <attr name="colorStroke" format="color"/>
        <attr name="idleStrokeWidth" format="dimension"/>
    </declare-styleable>
	
	<declare-styleable name="WaveVisualizer">
        <attr name="borderColor" format="color"/>
        <attr name="bgColor" format="color"/>
        <attr name="delimiterColor" format="color"/>
        <attr name="barsColor" format="color"/>
        <attr name="barWidth" format="dimension"/>
        <attr name="barsOffset" format="dimension"/>
    </declare-styleable>
</resources>